{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "test": {
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "now-build": {
      "outputs": [
        "{projectRoot}/build"
      ],
      "cache": true
    },
    "lint": {
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    }
  },
  "defaultBase": "master",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ]
}
